{
  "name": "flagstar",
  "version": "1.0.0",
  "type": "module",
  "description": "FlagStar 国旗之星 - 认识世界，从认识国旗开始 | Developed by UFlag team",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,html,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Freedom-Frank/UFlag.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Freedom-Frank/UFlag/issues"
  },
  "homepage": "https://github.com/Freedom-Frank/UFlag#readme",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vitest": "^4.0.7"
  },
  "dependencies": {
    "@types/three": "^0.181.0",
    "html2canvas": "^1.4.1",
    "three": "^0.181.0"
  }
}
